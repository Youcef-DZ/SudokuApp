{
  "name": "sudoku",
  "version": "1.0.0",
  "description": "Sudoku game compatible with local development",
  "proxy": "http://localhost:8080",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run web\"",
    "dev:expo": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web",
    "server": "node backend/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:hooks": "jest test/hooks",
    "type-check": "tsc --noEmit",
    "type-check:warn": "tsc --noEmit || echo '⚠️  TypeScript errors found (non-blocking for legacy code)'",
    "validate": "npm run type-check:warn && npm run test:hooks",
    "pre-deploy": "npm run validate && npx expo export --platform web"
  },
  "dependencies": {
    "@azure/cosmos": "^4.9.0",
    "@azure/identity": "^4.13.0",
    "@azure/msal-browser": "^4.27.0",
    "@azure/msal-react": "^3.0.23",
    "@descope/react-sdk": "^2.23.4",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/stack": "^7.6.13",
    "@types/styled-components": "^5.1.36",
    "babel-preset-expo": "^54.0.9",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "^54.0.30",
    "expo-blur": "^15.0.8",
    "expo-build-properties": "^1.0.10",
    "expo-font": "~14.0.10",
    "expo-haptics": "^15.0.8",
    "expo-linear-gradient": "^15.0.8",
    "expo-status-bar": "^3.0.9",
    "express": "^4.18.2",
    "fast-text-encoding": "^1.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets-core": "^1.6.2",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@expo/metro-config": "^54.0.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "jsdom": "^27.3.0",
    "react-refresh": "^0.18.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  }
}